windowFrameTopStyleSheet="QFrame#windowFrameTop {\n\tborder-left: 85px;\n\tpadding-left: -85px;\n\tborder-image: url(Skin/Greenery/frameTop.png);\n\tmin-height: 30px;\n\tmax-height: 30px;\n}"
